    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "mysql2": "^1.7.0",
    "passport": "^0.4.0",
    "prop-types": "^15.7.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
